{
  "version": "1.0",
  "name": "lambda_example",
  "func_name": "handler.lambda_handler",
  "resource_type": "lambda",
  "iam_role_name": "BasicExecutionRole",
  "runtime": "python3.10",
  "memory": 128,
  "timeout": 300,
  "lambda_path": "",
  "dependencies": [
    {
      "resource_name": "${userpool_name}",
      "resource_type": "cognito_idp"
    }
  ],
  "env_variables": {
    "cup_id": {
      "resource_name": "${userpool_name}",
      "resource_type": "cognito_idp",
      "parameter": "id"
    },
    "cup_client_id": {
      "resource_name": "${userpool_name}",
      "resource_type": "cognito_idp",
      "parameter": "client_id"
    }
  },
  "region": "${region}"
}