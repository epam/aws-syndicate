{
  "lambda_example_role": {
    "predefined_policies": [],
    "principal_service": "lambda",
    "custom_policies": [
      "lambda-example-policy"
    ],
    "resource_type": "iam_role",
    "allowed_accounts": [
      "${account_id}"
    ]
  },
  "lambda-example-policy": {
    "resource_type": "iam_policy",
    "policy_content": {
      "Version": "2012-10-17",
      "Statement": [
        {
          "Action": [
            "s3:Get*",
            "s3:List*"
          ],
          "Resource": "*",
          "Effect": "Allow"
        }
      ]
    }
  }
}
